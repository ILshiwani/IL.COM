<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Testimonial"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <cardsTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Cards Tab"
                    sling:resourceType="granite/ui/components/foundation/section">
                <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                        margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Heading"
                                    name="./heading"/>
                            <leftSection
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                    composite="{Boolean}true"
                                    fieldLabel="Testimonial Cards">
                                <field
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        name="./testimonialCards">
                                    <items jcr:primaryType="nt:unstructured">
                                        <leftSectionItem
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                            <items jcr:primaryType="nt:unstructured">
                                                <cardDesc
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                        fieldLabel="Card Description"
                                                        name="./cardDesc"/>
                                                <clientName
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Client Name"
                                                        name="./clientName"/>
                                                <clientPosition
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Client Position"
                                                        name="./clientPosition"/>
                                                <companyLogo
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Company Logo Path"
                                                        name="./companyLogo"/>
                                                <imgAlt
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Image ALT text"
                                                        name="./imgAlt"/>
                                            </items>
                                        </leftSectionItem>
                                    </items>
                                </field>
                            </leftSection>
                        </items>
                    </column>
                </items>
            </cardsTab>
            <bgIconsTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Background Icons Tab"
                    sling:resourceType="granite/ui/components/foundation/section">
                <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                        margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                                        <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <bgIcons
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        composite="{Boolean}true"
                                                        fieldLabel="Background Icons">
                                                    <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            name="./bgIcons">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <sectionItem
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/well">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <companyIcon
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                            fieldLabel="Company Icon Path"
                                                                            name="./companyIcon"/>
                                                                    <iconImgAlt
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            fieldLabel="Image ALT text"
                                                                            name="./iconImgAlt"/>
                                                                    <iconLink
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                            fieldLabel="Redirect Link Path"
                                                                            name="./iconLink"/>
                                                                    <slideNo
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                                            fieldLabel="Slide Number."
                                                                            name="./slideNo"/>
                                                                    <cssClass
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                            fieldLabel="Choose Class for position."
                                                                            name="./cssClass">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <one
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    text="Thumb One."
                                                                                    value="Thumb one active"/>
                                                                            <two
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    text="Thumb Two."
                                                                                    value="Thumb two"/>
                                                                            <three
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    text="Thumb Three."
                                                                                    value="Thumb three"/>
                                                                            <four
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    text="Thumb Four."
                                                                                    value="Thumb four"/>
                                                                            <five
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    text="Thumb Five."
                                                                                    value="Thumb five"/>
                                                                            <six
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    text="Thumb Six."
                                                                                    value="Thumb six"/>
                                                                        </items>
                                                                    </cssClass>
                                                                </items>
                                                            </sectionItem>
                                                        </items>
                                                    </field>
                                                </bgIcons>
                                            </items>
                                        </column>
                        </items>
                    </column>
                </items>
            </bgIconsTab>
        </items>
    </content>
</jcr:root>
